#: w.1:60
#, no-wrap
msgid "B<user >"
msgstr "B<Benutzer>"

#. type: Plain text
#: w.1:63
msgid "Show information about the specified user only."
msgstr "zeigt nur Informationen zum angegebenen Benutzer an."

