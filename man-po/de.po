#: vmstat.8:76
#, no-wrap
msgid "B<-t>, B<--timestamp>"
msgstr "B<-t>, B<--timestamp>"

#. type: Plain text
#: vmstat.8:79
msgid "Append timestamp to each line"
msgstr ""

